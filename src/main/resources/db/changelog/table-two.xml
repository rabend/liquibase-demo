<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.15.xsd">
    <changeSet id="table-two" author="roman.abendroth@rewe-digital.com">
        <createTable tableName="table_two">
            <column name="id" type="varchar(36)">
                <constraints primaryKey="true" />
            </column>
            <column name="title" type="text" />
            <column name="version" type="int" />
        </createTable>
    </changeSet>

</databaseChangeLog>
